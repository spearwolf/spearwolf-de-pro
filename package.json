{
  "name": "spearwolf-de-pro",
  "version": "0.0.1",
  "description": "the fantastic homepage of spearwolf",
  "license": "MIT",
  "author": {
    "name": "Wolfger Schramm",
    "email": "wolfger@spearwolf.de",
    "url": "http://spearwolf.de"
  },
  "scripts": {
    "build:webpack": "webpack --progress --colors",
    "build:webpack:release": "BUILD_DEV=0 webpack --progress --colors",
    "build:css": "mkdir -p .tmp && ./node_modules/node-sass/bin/node-sass index.scss --output-style compressed --include-path ./ --output .tmp/",
    "build:html": "./node_modules/jade/bin/jade.js index.jade --out ./ --path ./",
    "build:css:html": "npm run build:css && npm run build:html",
    "build": "npm run build:webpack && npm run build:css && npm run build:html",
    "release": "npm run clean && npm run build:webpack:release && npm run build:css && npm run build:html",
    "watch": "./node_modules/watch/cli.js 'npm run build:css:html' . --filter=watch.js --ignoreDotFiles --ignoreUnreadable",
    "start": "webpack-dev-server --inline --progress --colors",
    "clean": "rm -fr build .tmp"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-runtime",
      "transform-decorators",
      "transform-object-assign"
    ]
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^2.1.1",
    "jade": "^1.11.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "watch": "^0.17.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
